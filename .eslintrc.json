{
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"requireConfigFile": false,
		"babelOptions": {
			"plugins": ["@babel/plugin-syntax-import-assertions"]
		}
	},
	"env": {
		"node": true,
		"es6": true
	},
	"extends": ["airbnb/base", "prettier"],
	"rules": {
		"linebreak-style": 0,
		"arrow-parens": 0,
		"max-len": 0,
		"import/no-extraneous-dependencies": 0,
		"no-await-in-loop": 0,
		"no-console": 0
	}
}
